{
  "name": "@passageidentity/passage-react",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "0.5.0",
  "type": "module",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "main": "./dist/passage-react.umd.js",
  "module": "./dist/passage-react.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/passage-react.js",
      "require": "./dist/passage-react.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode $MODE",
    "build:watch": "vite build --mode $MODE --watch",
    "clean:dist": "rm -rf dist",
    "clean:node_modules": "rm -rf node_modules",
    "commit:pre": "npm run lint && npm run format",
    "format": "pretty-quick --staged",
    "lint": "eslint src --ext ts,tsx --max-warnings 50",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "log:env": "echo Using MODE: $MODE && echo Using APP_ENV: $APP_ENV",
    "test": "vitest run",
    "test:updateSnapshots": "vitest run -u",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@passageidentity/passage-elements": "1.14.0",
    "@passageidentity/passage-js": "3.8.1",
    "@types/lodash": "^4.14.195",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@imaginary-cloud/eslint-config-react": "^2.0.1",
    "@imaginary-cloud/prettier-config": "^1.0.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-passage": "*",
    "eslint-config-passage-react": "*",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
